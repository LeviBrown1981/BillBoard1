<h1 class="ui header">All Lists</h1>
<% @lists.each do |list| %>
  <h1><%= list.name %></h1>
  <%= link_to "Show", list_path(list) %>
  <%= link_to "Edit", edit_list_path(list) %>
  <%= link_to "Destroy", list_path(list), method: :delete %>
<% end %>
<br />
<%= link_to "New list", new_list_path %>

